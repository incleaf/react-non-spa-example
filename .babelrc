{
  "presets": ["es2015", "stage-3", "react"],
  "plugins": [
    "transform-es2015-destructuring",
    "transform-object-rest-spread"
  ],
  "env": {
      "development": {
        "plugins": [["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]]
      }
    }
}
